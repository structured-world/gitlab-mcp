{
  "name": "@structured-world/mcp-gitlab",
  "version": "2.1.0",
  "description": "MCP server for using the GitLab API",
  "license": "MIT",
  "author": "Dmitry Prudnikov <mail@polaz.com>",
  "contributors": [
    {
      "name": "zereight",
      "role": "original author"
    }
  ],
  "bin": "./dist/main.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node --no-warnings -r source-map-support/register --experimental-specifier-resolution=node dist/main.js",
    "dev:stdio": "node -r source-map-support/register -r ts-node/register --experimental-specifier-resolution=node -r ts-node/esm --experimental-print-required-tla --watch src/main.ts stdio",
    "dev:sse": "node -r source-map-support/register -r ts-node/register --experimental-specifier-resolution=node -r ts-node/esm --experimental-print-required-tla --watch src/main.ts sse",
    "watch": "tsc --watch",
    "test": "jest --testPathPatterns=tests/unit",
    "test:unit": "jest --testPathPatterns=tests/unit",
    "test:integration": "jest --testPathPatterns=tests/integration",
    "test:all": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" ",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\""
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@modelcontextprotocol/sdk": "^1.18.0",
    "@types/node-fetch": "^2.6.13",
    "express": "^5.1.0",
    "fetch-cookie": "^3.1.0",
    "form-data": "^4.0.4",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "node-fetch": "^3.3.2",
    "pino": "^9.9.5",
    "pino-pretty": "^13.1.1",
    "socks-proxy-agent": "^8.0.5",
    "tough-cookie": "^5.1.2",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "auto-changelog": "^2.5.0",
    "eslint": "^9.35.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "zod": "^3.25.76"
  }
}
